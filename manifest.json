{
  "manifest_version": 3,
  "name": "tabSort (by CodeSlayer)",
  "version": "0.0.1",
  "author": "Nathan Barnard",
  "description": "Sort your Chrome tabs with one click.",
  "homepage_url": "https://github.com/NathanBarnard/tabSort",
  "minimum_chrome_version": "88",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Sort Tabs",
    "default_icon": {
      "19": "assets/icon19.png",
      "38": "assets/icon38.png"
    }
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "128": "assets/icon128.png"
  }
}